   "param3" : "value",
   //
   "var1"   : "value 1 > ${param1}",
   "var2"   : "value 2 > ${param2}",
   "var3"   : "value 3 > ${param3}",
   //
   "var4"   : "value 4 > &{param1}",
   "var5"   : "value 5 > &{param2}",
   "var6"   : "value 6 > &{param3}",
   //
   "var7"   : "value 7 > @{param1}",
   "var8"   : "value 8 > @{param2}",
   "var9"   : "value 9 > @{param3}"
}
